{
    "name": "default-nodejs-project",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "clean": "rm -fr dist/",
        "copy-assets": "ts-node ./tools/copyAssets",
        "lint": "tslint -c tslint.json -p tsconfig.json --fix",
        "tsc": "tsc",
        "build": "yarn clean && yarn tsc && yarn copy-assets",
        "dev:start": "yarn build && yarn start",
        "dev": "nodemon --watch src -e ts --exec yarn dev:start",
        "start": "node .",
        "seed": "ts-node ./tools/seed",
        "es-status": "ts-node ./tools/esStatus"
    },
    "repository": "git@github.com:mp0120/default-backend-project.git",
    "author": "Vitali Korepanov",
    "license": "private",
    "private": true,
    "dependencies": {
        "@types/cors": "^2.8.6",
        "@types/dotenv": "^6.1.1",
        "@types/elasticsearch": "^5.0.36",
        "@types/express": "^4.17.0",
        "@types/fs-extra": "^8.0.0",
        "@types/node": "^12.6.8",
        "@types/shelljs": "^0.8.5",
        "cors": "^2.8.5",
        "dotenv": "^8.0.0",
        "elasticsearch": "^16.5.0",
        "express": "^4.17.3",
        "fs-extra": "^8.1.0",
        "nodemon": "^1.19.1",
        "npm-run-all": "^4.1.5",
        "rimraf": "^2.6.3",
        "shelljs": "^0.8.3",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3"
    }
}
